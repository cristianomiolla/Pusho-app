{
  "expo": {
    "name": "Pusho",
    "slug": "Pusho",
    "scheme": "pusho",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "owner": "pezzente",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.cristianomiolla.pusho",
      "buildNumber": "4",
      "icon": "./assets/icon-ios.png",
      "infoPlist": {
        "NSCameraUsageDescription": "Pusho ha bisogno della fotocamera per contare i tuoi push-up tramite riconoscimento della postura.",
        "NSLocationWhenInUseUsageDescription": "Pusho non utilizza la tua posizione.",
        "NSPhotoLibraryUsageDescription": "Pusho usa la libreria foto per creare share card personalizzate.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.cristianomiolla.pusho",
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Pusho ha bisogno della fotocamera per contare i tuoi push-up tramite riconoscimento della postura.",
          "enableMicrophonePermission": false
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Pusho usa le tue foto per creare share card personalizzate."
        }
      ],
      "expo-font",
      "./plugins/withMediaPipePreserve.js"
    ],
    "extra": {
      "eas": {
        "projectId": "ea901221-a265-48f4-9a7c-d49d45f23a40"
      }
    }
  }
}
